          }
        },
        "template--21129032892738__73e91363-55e8-4383-9c91-c145163b3c4f-collapsible_row-4": {
          "type": "collapsible_row",
          "settings": {
            "heading": "How often should I use it?",
            "icon": "check_mark",
            "row_content": "<p>It is advised to use the products once weekly for a minimum of 4 to 6 sessions.<\/p>",
            "page": ""
          }
        },
        "025414ff-094c-41d8-a191-933ca57127bb": {
